//Crea un arrow function llamada saludoPersonalizado que reciba dos parámettros: nombre y edad, retorne una cadena como la siguiente:
// "Hola, me llamo Isay y tengo 37 años"


const saludoPersonalizado = (nombre, edad) => {
  return "Hola, me llamo ${nombre} y tengo ${edad} años.";
};

console.log(saludoPersonalizado("Mariana", 20));