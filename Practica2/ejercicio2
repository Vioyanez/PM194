const productos = [
    {nombre: "Laptop", precio: 1000},
    {nombre: "Celular", precio: 250},
    {nombre: "Teclado", precio: 750},
    {nombre: "Monitor", precio: 3000}
];

//tu código aquí

//filtrar los productos que tienen un precio mayor a 1000   
const filtrados = productos.filter(producto => producto.precio >= 1000);

//map para crear un nuevo arreglo con los productos filtrados
const nombres = filtrados.map(p => p.nombre);

console.log(nombres); 